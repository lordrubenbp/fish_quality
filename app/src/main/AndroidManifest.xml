<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team3.fish_quality">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FormActivity"
            android:label="@string/title_activity_form"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ResultActivity"
            android:label="@string/title_activity_result"></activity>
        <activity android:name=".ReportActivity"
            android:label="@string/title_activity_report"></activity>

        <meta-data android:name="com.google.android.geo.API_KEY" android:value="AIzaSyBbgsEfU7oSerZIpWsHBt210l4RrdKA7qs"/>

    </application>

</manifest>